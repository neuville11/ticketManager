<p id="notice"><%= notice %></p>

<h1>Administratives</h1>
<%= paginate @administratives %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Cellphone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administratives.each do |administrative| %>
      <tr>
        <td><%= administrative.cellphone %></td>
        <td><%= link_to 'Show', administrative %></td>
        <td><%= link_to 'Edit', edit_administrative_path(administrative) %></td>
        <td><%= link_to 'Destroy', administrative, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @administratives %>
<br>

<%= link_to 'New Administrative', new_administrative_path, class: 'btn btn-success' %>
